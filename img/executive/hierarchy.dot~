digraph bureaucracy {
        overlap=false
        rankdir=BT

        head [shape=box, title="Coach"]
        oc [shape=box, title="Offensive Coordinator"]
        dc [shape=box, title="Defensive Coordinator"]
        offense [shape=box, title="Offensive Players"]
        defense [shape=box, title="Defensive Players"]

        oc -> head
        dc -> head
        offense -> oc
        defense -> dc
}
