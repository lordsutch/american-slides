digraph bureaucracy {
        overlap=false
        rankdir=BT

        head [shape=box, label="Coach"]
        oc [shape=box, label="Offensive Coordinator"]
        dc [shape=box, label="Defensive Coordinator"]
        offense [shape=box, label="Offensive Players"]
        defense [shape=box, label="Defensive Players"]

        oc -> head
        dc -> head
        offense -> oc
        defense -> dc
}
